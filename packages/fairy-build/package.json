{
  "name": "@fairy-render/build",
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch"
  },
  "bin": {
    "fairy": "./bin/fairy.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "devDependencies": {
    "@types/node": "^20.8.6",
    "tsup": "^8.1.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@fairy-render/vite-plugin": "workspace:^",
    "@vitejs/plugin-react": "^4.3.1",
    "commander": "^12.1.0",
    "vite": "^5.3.1",
    "vite-plugin-solid": "^2.10.2"
  }
}
