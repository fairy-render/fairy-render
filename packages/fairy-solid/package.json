{
  "name": "@fairy-render/solid",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./server": {
      "import": "./dist/server.js",
      "require": "./dist/server.cjs",
      "types": "./dist/server.d.ts"
    }
  },
  "devDependencies": {
    "@types/node": "^20.8.6",
    "tsup": "^8.1.0",
    "typescript": "^5.2.2",
    "solid-js": "^1.9.3"
  },
  "peerDependencies": {
    "solid-js": "^1.9.3"
  }
}
